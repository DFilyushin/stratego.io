.setup-panel.panel
.setup-grid
  - for (var y = 0; y < 4; y++) {
    - for (var x = 0; x < 10; x++) {
      .cell(data-x=x, data-y=y)
    - }
  - }
  - for (var i = 0; i < 3; i++) {
    .horizontal-grid-line(data-number=i)
  - }
  - for (var i = 0; i < 9; i++) {
    .vertical-grid-line(data-number=i)
  - }
